<?xml version="1.0" encoding="UTF-8"?>
<plugin xmlns="http://apache.org/cordova/ns/plugins/1.0"
    xmlns:android="http://schemas.android.com/apk/res/android"
    id="notificationmanager"
    version="0.2.11">
    <name>NotificationManager</name>
    <description>Notificacion en barra de estado</description>
    <license>Apache 2.0</license>
    <keywords>cordova,coolest</keywords>
    <repo></repo>
    <issue></issue>
    <js-module src="www/NotificationManager.js" name="NotificationManager">
        <clobbers target="NotificationManager" />
    </js-module>
    <!-- android -->
    <platform name="android">
        <config-file target="res/xml/config.xml" parent="/*">
            <feature name="NotificationManager" >
                <param name="android-package" value="NotificationManager"/>
            </feature>
        </config-file>
        <source-file src="src/android/NotificationManager.java" target-dir="src/" />
    </platform>
</plugin>